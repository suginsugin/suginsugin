#include<stdio.h>
int main()
{
	int i,fact=1,n;
	printf("suginbalaji G\n192321111\n");
	printf("enter the number: ");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		fact=fact*i;
	}
	printf("the factorial of %d:%d",n,fact);
	return 0;
}
