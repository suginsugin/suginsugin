#include<stdio.h>
int main()
{
	printf("suginbalaji\n192321111\n");
	int a[100],n;
	printf("Enter array Size\n");
	scanf("%d",&n);
	printf("Enter array element\n");
	for(int i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Even element in Array are:\n");
	for(int i=0;i<n;i++)
	{
		if(a[i]%2==0){
			printf("%d ",a[i]);
		}
	
	}
	printf("\nOdd element in Array are:\n");
	for(int i=0;i<n;i++)
	{
		if(a[i]%2!=0){
			printf("%d ",a[i]);
		}
	
	}
}
