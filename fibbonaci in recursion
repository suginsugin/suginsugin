#include <stdio.h>
int fibonacci(int n) 
{
    if (n <= 1) 
    {
        return n;
    }
    return fibonacci(n-1) + fibonacci(n-2);
}

int main() 
{
	
    int n =12;
    printf("suginbalaji G\n192321111\n");
    printf("Fibonacci number at position %d is :%d", n, fibonacci(n));
    return 0;
}
