#include<stdio.h>
#include<stdlib.h>
int main()
{
	int a[]={10,20,30,40};
	int b[]={10,20,30,40,50,};
  int top1=(sizeof(a)/sizeof(a[0])-1);
 int top2=(sizeof(b)/sizeof(b[0])-1);
	printf("sum of top and bottom elements\n");
	printf("stack1:%d\n",a[0]+a[top1]);
	printf("stack2:%d\n",b[0]+b[top2]);
	printf("suginbalaji\n");
	printf("192321111\n");
}
